{
  "name": "rn-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android --active-arch-only",
    "android:staging": "cd ./android && cross-env ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:prod": "cd ./android && cross-env ENVFILE=.env.prod ./gradlew app:assembleRelease",
    "android:publish": "cd ./android && fastlane publish",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "lint": "eslint 'src/**/*.{js,jsx,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write --cache --cache-strategy metadata 'src/**/*.{js,jsx,ts,tsx}'",
    "clean": "react-native-clean-project",
    "postinstall": "npx husky install && npx patch-package",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "pod:debug": "cd ios && fastlane poddebug && cd ..",
    "pod:release": "cd ios && fastlane podrelease && cd ..",
    "ios:publish": "cd ./ios && fastlane publish",
    "codepush": "node ./scripts/codePush/index.js"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.5",
    "@ebay/nice-modal-react": "^1.2.10",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@sdcx/image-crop": "^0.2.0",
    "@sdcx/keyboard-insets": "^0.1.0",
    "@sdcx/nested-scroll": "^0.4.0",
    "@sdcx/pull-to-refresh": "^0.7.0",
    "@shopify/flash-list": "^1.5.0",
    "@td-design/react-native": "^5.0.1",
    "@td-design/react-native-image-picker": "^1.6.8",
    "@td-design/react-native-picker": "^2.2.1",
    "@td-design/react-native-tabs": "^4.1.0",
    "@td-design/rn-hooks": "^2.7.3",
    "@wuba/react-native-echarts": "^1.2.2",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "expo": "^49.0.5",
    "expo-image": "~1.3.2",
    "immer": "^10.0.2",
    "jotai": "^2.2.2",
    "lodash-es": "^4.17.21",
    "lottie-react-native": "5.1.6",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-native": "0.72.3",
    "react-native-bars": "^2.2.2",
    "react-native-blob-util": "^0.18.4",
    "react-native-bootsplash": "^4.7.5",
    "react-native-code-push": "^8.0.2",
    "react-native-config": "^1.5.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-mmkv": "^2.10.1",
    "react-native-pager-view": "^6.2.0",
