{
  "name": "luneng",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "clean": "cd ./android && ./gradlew clean",
    "build": "npm run clean && npm run jetifier && cd ./android && ./gradlew assembleRelease",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint": "eslint --ext .ts,.tsx src/",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint": "prettier --check src/**/*.ts src/**/*.tsx",
    "lint:fix": "prettier --check src/**/*.tsx --write",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "release": "git add . && node release.js",
    "postrelease": "git push --follow-tags",
    "jetifier": "npx jetifier",
    "pod": "cd ios && pod install && cd .."
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.0",
    "@ant-design/icons-react-native": "1.0.2",
    "@ant-design/react-native": "3.2.2",
    "@ice/store": "1.3.0",
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^5.7.0",
    "@react-navigation/drawer": "^5.4.0",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "@umijs/use-request": "^1.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rc-field-form": "^1.1.1",
    "react-native": "0.61.5",
    "react-native-canvas": "^0.1.37",
    "react-native-device-info": "^5.5.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.4.0",
    "react-native-modal": "^11.5.6",
    "react-native-orientation-locker": "^1.1.8",
    "react-native-permissions": "^2.0.10",
    "react-native-ratings": "^6.5.0",
    "react-native-reanimated": "^1.7.1",
    "react-native-root-toast": "^3.2.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.4.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.0.3",
    "react-native-switch": "^1.5.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "8.1.2",
    "rn-fetch-blob": "^0.12.0",
    "umi-request": "^1.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/echarts": "4.4.3",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/react-native": "^0.62.1",
    "@types/react-native-canvas": "^0.1.3",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.2.4",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jetifier": "^1.6.5",
    "lint-staged": "^10.1.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "pont-engine": "^1.0.3",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
