  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@bam.tech/react-native-make": "^3.0.3",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "16.0.0",
