    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@shopify/restyle": "^1.6.1",
    "@td-design/react-native": "^3.10.0",
    "@td-design/react-native-echarts": "^1.4.0",
    "@td-design/react-native-image-picker": "^1.5.5",
    "@td-design/react-native-picker": "^1.7.0",
    "@td-design/rn-hooks": "^2.2.7",
    "dayjs": "^1.10.7",
    "immer": "^9.0.12",
    "jotai": "^1.5.3",
    "jotai-modular": "^1.3.1",
    "lodash-es": "^4.17.21",
    "rc-field-form": "^1.22.0",
    "react": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-native": "^0.66.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^4.1.0",
    "react-native-config": "^1.4.5",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.7.1",
